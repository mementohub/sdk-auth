{
    "name": "imemento/sdk-auth",
    "description": "SDK for iMemento Auth",
    "homepage": "https://github.com/mementohub/sdk-auth",
    "license": "MIT",
    "authors": [
        {
            "name": "George Stan",
            "email": "contact@georgestan.com",
            "homepage": "http://imemento.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "imemento/jwt": "~8.1.0",
        "imemento/support": "~8.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "iMemento\\SDK\\Auth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iMemento\\SDK\\Auth\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
