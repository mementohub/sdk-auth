{
    "name": "imemento/sdk-auth",
    "description": "SDK for iMemento Auth",
    "homepage": "https://github.com/mementohub/sdk-auth",
    "license": "MIT",
    "authors": [
        {
            "name": "George Stan",
            "email": "contact@georgestan.com",
            "homepage": "http://imemento.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "laravel/framework": "*",
        "guzzlehttp/guzzle": "*",
        "imemento/jwt": "dev-master",
        "imemento/support": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "iMemento\\SDK\\Auth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iMemento\\SDK\\Auth\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}
